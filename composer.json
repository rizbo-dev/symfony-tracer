{
    "name": "rizbo/symfony-tracer",
    "description": "Symfony bundle that traces request and log actions for tracing",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rizbo\\SymfonyTracer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Boris Evetovic"
        }
    ],
    "require": {
        "symfony/event-dispatcher": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/config": "^7.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2"
    }
}
